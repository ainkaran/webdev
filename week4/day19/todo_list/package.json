    "db:setup": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "morgan": "^1.8.2",
    "pg-promise": "^6.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
